// Input parameters for the model
// Legal syntax is key : value
// You can end input key:value lines with comments

simLength  : 259200            // How long to simulate [s]
cfl        : 0.5              // CFL value to use
outFreq    : 3600              // Frequency for spatial output [s]
obsFreq    : 3600              // Frequency for temporal series output [s]
parNx      : 1                // Number of parallel tasks in the x-direction
parNy      : 1                // Number of parallel tasks in the y-direction
outFormat  : NETCDF		//Output format: NETCDF or VTK
BCtype     : REFLECTIVE    //outer boundaries: TRANSMISSIVE, REFLECTIVE or PERIODIC
